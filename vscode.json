    "atlascode.jira.jqlList": [
        {
            "enabled": true,
            "filterId": "10502",
            "id": "82bddd68-5b48-4a2b-83e4-1112be613579",
            "monitor": true,
            "name": "Reported by Me & Unresolved Across All Projects",
            "query": "resolution = Unresolved AND reporter in (currentUser()) ORDER BY created DESC, priority DESC",
            "siteId": "85e7534e-7a93-40f4-a71e-5204ece7ec46"
        },
        {
            "enabled": true,
            "id": "54f1d841-860a-44dd-8cc5-ff9ecff14b34",
            "monitor": true,
            "name": "My moengagetrial Issues",
            "query": "assignee = currentUser() AND resolution = Unresolved ORDER BY lastViewed DESC",
            "siteId": "85e7534e-7a93-40f4-a71e-5204ece7ec46"
        },
        {
            "enabled": true,
            "filterId": "10503",
            "id": "396e1444-d2ff-4e46-be3b-3ba452e3643c",
            "monitor": true,
            "name": "Data Team - Unresolved Support Tickets",
            "query": "project = SUP AND status in (DevComplete, IN-PROGRESS, InQA, \"QA Completed\", Reopened, TODO) AND resolution = Unresolved AND assignee in (5add914deee87f2e31512bb5, 5e12fdd2cbf1830daa9ce632, 5cd2afdbd210950fd02d08f1, 5c988c3e2116a07570791714, 5ae1705f87ac7129b88e95dd, 5fa52d52b38e610071525f58, 5fe1daa4dd5eb50108429d92) ORDER BY created DESC, cf[10049] ASC, component DESC, priority DESC",
            "siteId": "85e7534e-7a93-40f4-a71e-5204ece7ec46"
        },
        {
            "enabled": true,
            "filterId": "11085",
            "id": "399f2d91-9562-4ce5-bc58-fb866c33a05c",
            "monitor": true,
            "name": "Action Streaming Bugs",
            "query": "project = DT AND status not in (\"Not a Bug\", Released, Closed, \"QA Completed\", \"Tech Limitation(Cannot fix)\") AND labels = SDP AND component = sdp-qa ORDER BY status DESC, created DESC",
            "siteId": "85e7534e-7a93-40f4-a71e-5204ece7ec46"
        }
    ],
